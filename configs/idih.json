{
  "model": {
    "type": "DIH",
    "base": {},
    "backbone": {
      "use": false
    }
  },
  "trainer": {
    "type": "SupervisedTrainer",
    "restore_scale": true,
    "sync_bn": true
  },
  "optimizers": {
    "model_optimizer": {
      "type": "AdamW",
      "groups": [
        {
          "params": "base",
          "lr": 2e-3,
          "weight_decay": 1e-4
        }
      ]
    }
  },
  "criterions": {
    "MSE": {
      "type": "MSECriterion"
    },
    "PSNR_255": {
      "type": "PSNRCriterion",
      "max_pixel_value": 255.0
    },
    "fMSE": {
      "type": "fMSECriterion"
    },
    "FNMSE": {
      "type": "FNMSECriterion",
      "min_area": 100.0
    }
  },
  "metric_callbacks": [
    {
      "type": "CriterionCallback",
      "input_key": "outputs_255",
      "target_key": "targets_255",
      "metric_key": "MSE_255",
      "criterion_key": "MSE"
    },
    {
      "type": "CriterionCallback",
      "input_key": "outputs_255",
      "target_key": "targets_255",
      "metric_key": "PSNR_255",
      "criterion_key": "PSNR_255"
    },
    {
      "type": "CriterionCallback",
      "input_key": "outputs_255",
      "target_key": "targets_and_masks_255",
      "metric_key": "fMSE_255",
      "criterion_key": "fMSE",
      "eval_only": true
    },
    {
      "type": "CriterionCallback",
      "input_key": "outputs",
      "target_key": "targets_and_masks",
      "metric_key": "loss_FNMSE",
      "criterion_key": "FNMSE"
    }
  ],
  "optimizers_callbacks": [
    {
      "metric_key": "loss_FNMSE",
      "model_key": "model",
      "optimizer_key": "model_optimizer"
    }
  ],
  "checkpoints_callbacks": [
    {
      "use": true,
      "loader_key": "valid",
      "metric_key": "PSNR_255",
      "minimize": false,
      "save_n_best": 1
    }
  ],
  "schedulers": {
    "model_optimizer": {
      "type": "MultiStepLR",
      "milestones": [160, 175],
      "gamma": 0.1,
      "mode": "epoch"
    }
  },
  "data": {
    "dataset_paths": {
      "HCOCO": "../data/HCOCO",
      "HAdobe5k": "../data/HAdobe5k_resized1024",
      "HFlickr": "../data/HFlickr",
      "Hday2night": "../data/Hday2night",
      "RealHM": "../data/RealHM_resized1024",

      "MSCOCO": "../data/MSCOCO/train/data",
      "UnsplashLite": "../data/UnsplashLite/data",
      "UnsplashFull": ""
    },
    "train": {
      "datasets": ["HCOCO", "HAdobe5k", "Hday2night", "HFlickr"],
      "type": "HDataset",
      "crop": {
        "PadIfNeeded": true,
        "type": "RandomResizedCrop",
        "height": 256,
        "width": 256,
        "scale": [0.5, 1.0]
      },
      "augmentations": {
        "HorizontalFlip": {
          "p": 0.5
        }
      },
      "additional_targets": {
        "target": "image"
      }
    },
    "test": {
      "datasets": ["HCOCO", "HAdobe5k", "Hday2night", "HFlickr"],
      "type": "HDataset",
      "augmentations": {
        "Resize": {
          "height": 256,
          "width": 256
        }
      },
      "additional_targets": {
        "target": "image"
      }
    },
    "batch_size": 100,
    "num_workers": 40
  },
  "gpus": "",
  "num_epochs": 180,
  "project_name": "gml_harm",
  "experiments_folder": "../diplom_exps"
}